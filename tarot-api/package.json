    "@babel/preset-env": "^7.12.1",
    "body-parser": "^1.18.2",
    "cors": "^2.8.5",
    "express": "^4.16.2",
    "fs": "0.0.1-security",
    "lodash.clonedeep": "^4.5.0",
    "lodash.range": "^3.2.0",
    "lodash.remove": "^4.7.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "nodemon": "^2.0.16"
